{
  "name": "ghp-portail",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "description": "GHP Portail - Plateforme d'entreprise moderne avec modules intégrés et interface intuitive",
  "keywords": [
    "react",
    "enterprise-portal",
    "business-modules",
    "team-collaboration",
    "dashboard",
    "material-ui",
    "scss",
    "fake-api"
  ],
  "author": "GHP Portail Team",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,scss,css}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,scss,css}\"",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:perf": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "clean": "rimraf dist node_modules/.vite",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "migrate": "node scripts/migrateData.js",
    "deploy": "bash scripts/deploy.sh",
    "postinstall": "npm run lint:fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.10",
    "@mui/x-data-grid": "^6.0.0",
    "@mui/x-date-pickers": "^6.20.2",
    "axios": "^1.3.4",
    "clsx": "^1.2.1",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.0.1",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.5",
    "react-hot-toast": "^2.4.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.8.1",
    "recharts": "^2.15.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/lodash-es": "^4.17.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.1",
    "eslint": "^8.36.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "playwright": "^1.30.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "rimraf": "^4.1.2",
    "sass": "^1.58.3",
    "vite": "^4.1.0",
    "vite-bundle-analyzer": "^0.7.0",
    "vitest": "^0.29.8"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{scss,css,json,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ghp-portail.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/ghp-portail/issues"
  },
  "homepage": "https://github.com/yourusername/ghp-portail#readme"
}
